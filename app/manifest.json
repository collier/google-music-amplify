{
  "name": "Google Music Amplify",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "A chrome extension that adds features to Google Music for fetching metadata",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "Google Music Amplify"
  },
  "permissions": [
    "declarativeContent"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://play.google.com/music/*",
        "https://play.google.com/music/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.min.js",
        "bower_components/materialize/dist/js/materialize.min.js",
        "bower_components/rivets/dist/rivets.bundled.min.js",
        "modules/utils/utils.js",
        "modules/metadata/metadata.js",
        "modules/metadata/modal/modal.js",
        "google-music-amplify.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "bower_components/materialize/dist/*",
    "images/*",
    "modules/*"
  ]
}
